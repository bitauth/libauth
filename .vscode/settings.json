{
  "cSpell.userWords": [], // only use words from .cspell.json
  "deno.enable": false,
  "editor.formatOnSave": true,
  "editor.semanticHighlighting.enabled": true,
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
